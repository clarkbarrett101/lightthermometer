{
  "expo": {
    "name": "light-thermometer",
    "slug": "light-thermometer",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/app_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.clarkbarrett101.light-thermometer",
      "infoPlist": {
        "NSCameraUsageDescription": "Light thermometer needs access to your camera to take real time color measurements."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.CAMERA"],
      "package": "com.clarkbarrett101.lightthermometer"
    },
    "web": {
      "favicon": "./assets/app_icon.png"
    },
    "plugins": [
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Light thermometer needs access to your camera to take real time color measurements"
        }
      ],
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Light thermometer needs access to your camera to take real time color measurements"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1e37c755-0259-4c28-afe6-d2ec8c4f2649"
      }
    }
  }
}
